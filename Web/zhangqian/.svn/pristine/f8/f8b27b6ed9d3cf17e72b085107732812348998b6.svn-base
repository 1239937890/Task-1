* {
    padding: 0;
    margin: 0;
}
p{
    margin: 0;
}
header nav.head {
    width: 100%;
    height: 3em;
    border: none;
    min-height: 0;
    position: fixed;
    background-color: #29bde0;
}
ul.head-ul {
    margin: 0;
    display: flex;
}
ul.head-ul li {
    flex: 1;
    float: left;
    list-style-type: none;
    text-align: center;
}
li .head-middle {
    padding: 0;
    margin: .25em 0 0;
    font-size: 1.5em;
    display: inline-block;
    color: #fff;
}
nav ul li img {
    height: 2em;
    width: 2em;
    vertical-align: middle;
}
li .head-left-img {
    margin: .5em 0 0 .5em;
     float: left;
 }
li .head-right-img {
    margin: .5em .5em 0 0;
    float: right;
}
.head-player {
    height: 2em;
    width: 2em;
    border:none;
    border-radius: 0;
    background-image: url("p1.3.png");
    background-color: transparent;
    background-size: 2em 2em;
    background-repeat: no-repeat;
    float: right;
    margin: .5em 1em ;
    /*position: fixed;*/
    /*top: 3.5em;*/
    /*right: .5em;*/
    /*z-index: 999;*/
}
main div.main {
    width: 100%;
    height: auto;
    position: absolute;
    top: 3em;
    bottom: 3.5em;
    left: 0;
    overflow-x: hidden;
    overflow-y: auto;
    background-color: #29bde0;
}
main div.main::-webkit-scrollbar {
    width:0;
}
main div.main-vote {
    height: 3em;
    background-color: #b9e9f5;
}
div.main-vote p {
    line-height: 3em;
    margin: 0 0 0 1em;
    display: inline-block;
}
div.main-body p {
    color: #fff;
    margin: .5em 0 0 2em;
}
div.main-body span {
    top: 3em;
    left: 1.5em;
    border-top: .8em;
    border-right: .5em;
    border-left: .5em;
    border-color: #b9e9f5 transparent transparent transparent;
    border-style: solid dashed dashed;
    position: absolute;
}
div.container div.row div {
    border: none;
    text-align:center;
    /*display: inline-block;*/
}
.col-xs-4 {
    height: 30vw;
    margin: .5em 0 0 0;
    text-align: center;
}
div.main-content {
    display: inline-block;
}
div.content {
    height: 23vw;
    width: 23vw;
    padding-top: .5vw;
    background-color: #fff;
}
div.content-body {
    height: 22vw;
    width: 22vw;
    padding: 0;
    margin: 0 .5vw .5vw;
    background-color: #f5c97b;
    display: block;
}
div.content-status {
    height: 16vw;
    width: 22vw;
    line-height: 15vw;
    margin: 0;
    padding: 0;
    display: inline-block;
    background-color: #f5c97b;
    border: none;
}
div.content-status>p {
    font-size: 6vw;
}
div.content-num {
    height: 6vw;
    width: 22vw;
    line-height: 6vw;
    display: inline-block;
    background-color: #83b09a;
}
div.content-num>p {
    font-size: 4vw;
}
div.signs {
    height: 5vw;
    width: 23vw;
    margin: 2vw 0 0 0;
    display: none;
    /*float: left;*/
}
/*div #signs-img {*/
    /**/
/*}*/
div.signs img {
    padding: 0;
    height: 5vw;
    width: 5vw;
    background-color: #fff;
    float: left;
}
img.img1 {
    margin: 0 1vw 0 0;
}
div.content:hover + .signs {
    display: block;
}
footer nav.foot {
    width: 100%;
    height: 3em;
    border: none;
    background-color: #29bde0;
}
nav.foot button {
    height: 2.5em;
    width: 80%;
    padding: 0;
    margin: .5em 10%;
    border-style: none;
    border-radius: 0;
    background-color: #fbb435;
}
nav.foot button p {
    margin: 0;
    color: #fff;
    text-decoration: none;
    font-size: 1.5em;
    display: inline-block;
}