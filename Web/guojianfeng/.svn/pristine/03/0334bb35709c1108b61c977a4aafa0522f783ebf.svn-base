/*轮播图*/
.carousel-inner>.item>img {
    width:100%;
}
/*宣传简介*/
.summary {
    text-align: center;
    padding:50px 0;
}
#column p {
    text-align: left;
}
.contactlogo {
    height: 70px;
    text-align: center;
}
.data {
    color: #999;
    font-size: 18px;
}
.data ul {
    margin: 15px 0;
    padding: 0;
}
.data li {
    list-style: none;
}
.data span {
    color: #26a26f;
    margin-left: 25px;
}
/*学习方法*/
.font14px {
    font-size: 14px;
    color: #999;
}
.learn {
    padding:40px 0;
    text-align: center;
}
.round {
    height: 70px;
    width: 70px;
    border-radius: 50%;
    box-shadow: 0 1px 4px #999;
    line-height: 70px;
    display: inline-block;
    background-color: #fff;
    margin: 15px 15px 15px 0;
}
.round p {
    font-size: 18px;
    color: #29b078;
    font-weight: 700;
    text-align: center;
}
.textvertical {
    width:40%;
    display: inline-block;
    vertical-align: middle;
}
span img {
    margin: 30px 15px 25px 0;
}
/*学员展示*/
.learner {
    padding:40px 0;
}
.resume {
    background-color: #fff;
    box-shadow: 2px 3px 10px #f1f1f1;
    text-align: center;
}
.logobox {
    margin: 15px auto;
}

.page {
    height: 55px;
    padding: 20px 0;
    width: 100%;
    float: left;
    text-align: center;
}
.pagebtn {
    width: 15px;
    height: 15px;
    border-radius: 50%;
    display: inline-block;
    background-color: #fff;
    box-shadow: 2px 3px 10px #e2e2e2;
}
.pagebtn:nth-child(1):hover  {
    background-color: #29b078;
}
.pagebtn:nth-child(2):hover  {
    background-color: #29b078;
}
.pagebtn:nth-child(3):hover  {
     background-color: #29b078;
 }
.pagebtn:nth-child(4):hover  {
      background-color: #29b078;
  }


/*合作伙伴*/
.partner {
    text-align: center;
    margin-top: 70px;
    box-shadow: 1px 1px 10px #f1f1f1;
}
.partnerbox {

}
.part {
    display: inline-block;
    text-align: center;
    vertical-align: middle;
    line-height: 170px;
    width: 100%;
}
.alibaba,.jinshanyun,.huanxin,.ronglian,.qiniu {
    background-color: #fff;
    margin-bottom: 10px;
}
.alibaba:hover {
    box-shadow: 0 0 10px #111;
}
.jinshanyun:hover {
    box-shadow: 0 0 10px #111;
}
.huanxin:hover {
    box-shadow: 0 0 10px #111;
}
.ronglian:hover {
    box-shadow: 0 0 10px #111;
}
.qiniu:hover {
    box-shadow: 0 0 10px #111;
}
/*合作链接*/
.link {
    margin-top: 60px;
    background-color: #fff;
    text-align: center;
}
.link ul {
    padding:0;
}
.link li {
    float:left;
    color: #999;
    font-size: 15px;
    margin: 10px 0 0 60px;
}
.link li a {
    color: #999;
}
@media (min-width: 560px) {
    .textvertical {
        width: 70%;
    }
    .link p {
        margin:10px 0 0 20px;
    }
}
@media (max-width: 768px) {
    .learn, {
        text-align: center;
    }

}
@media (min-width: 992px) {
    .font16px,.data {
        text-align: center;
    }
    .data {
        border:none;
    }
    .round {
        height: 55px;
        width: 55px;
        border-radius: 50%;
        box-shadow: 0 1px 4px #999;
        line-height: 55px;
        background-color: #fff;
        margin: 15px 0 15px 0;
    }
    .round p {
        font-size: 18px;
        color: #29b078;
        font-weight: 700;
        text-align: center;
    }
    .link p {
        margin:10px 0 0 30px;
    }
}



@media (min-width:1200px) {
    .data {
        border-left: dashed #e2e2e2;
        text-align: left;
        padding-left: 30px;
    }
    .font16px {
        text-align: left;
    }
    .textvertical {
        width:51%;
    }
    .link li {
        margin: 10px  38px 0 20px;
    }
    .partnerbox{
        width: 20%;
    }
}
