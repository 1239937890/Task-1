@color:#29b078;
.learner {
  padding: 50px;
  .resume {
    background-color: #fff;
    box-shadow: 2px 3px 10px #f1f1f1;
    text-align: center;
    padding: 20px 0;
    margin: 20px auto;
    .logobox {
      margin: 30px auto;
    }
    .f18 {
      color: #000;
      font-weight: 700;
      padding-bottom: 20px;
    }
  }
}

.page {
  height: 55px;
  width: 100%;
  padding: 20px 0;
  float: left;
  text-align: center;
  .pagebtn {
    width: 15px;
    height: 15px;
    border-radius: 50%;
    display: inline-block;
    background-color: #fff;
    box-shadow: 2px 3px 10px #e2e2e2;
    &:nth-child(1):hover {
      background-color: @color;
    }
    &:nth-child(2):hover {
      background-color: @color;
    }
    &:nth-child(3):hover {
      background-color: @color;
    }
    &:nth-child(4):hover {
      background-color: @color;
    }
  }
}