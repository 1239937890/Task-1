main {
  padding-bottom: 50px;
}
.content {
  background-color: #fff;
  box-shadow: 0 1px 10px #e1e1e1;
}
.content .brief {
  padding: 30px;
  border-bottom: 1px solid #e6e6e6;
}
.content .detail {
  padding: 30px;
}
.content .f16 {
  color: #999;
  line-height: 25px;
  text-indent: 32px;
}
.ver-nav {
  box-shadow: 0 1px 10px #e1e1e1;
}
.ver-nav .title {
  padding-top: 10px;
  text-align: center;
  border-bottom: 1px solid #e6e6e6;
}
.ver-nav .title h4 {
  font-weight: 700;
}
.ver-nav li {
  list-style: none;
  margin: 10px auto;
  padding: 20px 0;
  width: 95px;
}
.ver-nav li .circle {
  height: 13px;
  width: 13px;
  border-radius: 50%;
  border: 1px solid orange;
  display: inline-block;
  vertical-align: middle;
  margin-right: 5px;
}
.ver-nav li a {
  color: #29b078;
}
