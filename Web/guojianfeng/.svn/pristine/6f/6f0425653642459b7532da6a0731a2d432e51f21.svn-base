header{
  display:flex;
  justify-content: space-between;
  h4{
    margin:0;
  }
}
main {
  top: 4.2rem;
  bottom:.1vw;
  padding-bottom: 16.4vw;
  background-color:#29bde0;
  .win {
    border-top: 1px solid #74d4eb;
    padding: 4vw 0;
    text-align: center;
    img {
      height: 55%;
      width: 55%;
    }
  }
  .victory {
    width: 75vw;
    margin: 0 12.5vw 3vw 12.5vw;
    .yellowfont {
      color: #ffc865;
      font-size: 4.5vw;
      text-align: justify;
    }
    .gameplayer {
      padding:0;
      text-align: left;
      margin-top: 6.5vw;
      li {
        display: inline;
        margin-right:7vw;
        p {
          color: #fff;
          font-size: 3.5vw;
          display: inline;
        }
      }
    }
  }
  .box {
    border-bottom: 1px solid #dddddd;
    background-color: white;
    p {
      margin: 0 12vw;
      line-height: 2;
      font-size: 4.4vw;
      color: #a6a6a6;
      .big {
        font-size: 3.2vw;
      }
      .time {
        float: right;
      }
    }
  }
}
footer {
  display:flex;
  background-color: rgba(153,223,240,0.7);
  a {
    width: 50%;
    border-radius: 3vw;
    font-size: 5vw;
    color: #fff;
    margin: 2vw 4vw;
    padding:3vw;
    text-decoration: none;
    justify-content: center;
    align-items: center;
    display: flex;
  }
  .footer-left {
    background-color: #f66f6f;

  }
  .footer-right {
    background-color: #fbb435;
  }
}