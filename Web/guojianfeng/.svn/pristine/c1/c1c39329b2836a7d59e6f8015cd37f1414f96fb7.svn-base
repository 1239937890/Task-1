.green {
  color: #29b078;
}
.bar {
  margin: 0 8px;
}
.nav {
  background-color: #29b078;
  padding-left: 0;
  list-style: none;
}
.nav .navbar {
  background-color: #29b078;
  min-height: 50px;
  position: relative;
  margin-bottom: 20px ;
}
.nav .navbar-default {
  margin-bottom: 0;
}
.nav .navbar-default .navbar-nav {
  float: left;
  margin: 0;
}
.nav .navbar-default .navbar-nav > li {
  float: left;
}
.nav > li,
.nav > li > a {
  position: relative;
  display: block;
}
.nav > li > a {
  padding: 10px 15px;
  color: #fff;
  line-height: 20px;
}
nav {
  padding-left: 15px;
}
.nav-box {
  border-bottom: 3px solid #fff;
}
.nav-box a {
  font-size: 1.5rem;
}
@media (max-width: 768px) {
  .hidden {
    display: none;
  }
}
@media (min-width: 768px) {
  .navvbar-right {
    float: right !important;
    margin-right: -15px;
  }
  .navbar-nav {
    float: left;
    margin: 0;
  }
  .navbar-nav > li {
    float: left;
  }
  .navbar-nav > li > a {
    padding-top: 15px;
    padding-bottom: 15px;
  }
}
.banner img {
  width: 100%;
}
.sitemap {
  line-height: 80px;
  background-color: #fafafa;
  font-size: 1.6rem;
}
.sitemap a {
  color: #000;
}
.sitemap span {
  color: #29b078;
}
footer {
  background-color: #29b078;
  color: #fff;
}
footer .footbox {
  padding: 20px -15px;
}
footer .footbox p {
  font-size: 1.5rem;
}
footer .footbox a {
  font-size: 1.4rem;
  display: inline-block;
  color: #fff;
}
footer .copyright {
  text-align: center;
  border-top: 1px solid #32c086;
  font-size: 1.4rem;
  line-height: 30px;
}
@media (min-width: 768px) {
  .footbox {
    padding: 20px 0;
  }
}
* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
html,
body {
  width: 100%;
  height: 100%;
  font-size: 62.5%;
}
body {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 62.5%;
  line-height: 1.42857143;
  color: #333;
  background-color: #fafafa;
}
h3 {
  text-align: center;
  font-weight: 700;
  clear: both;
}
