.summary {
  padding: 50px 0;
  text-align: center;
}
.summary .contactlogo {
  text-align: center;
  height: 70px;
}
.summary .contactlogo .black {
  color: #000;
  font-weight: 700;
  padding: 10px 0;
}
.summary ul {
  margin: 15px 0;
}
.summary ul li {
  list-style: none;
}
.f16,
.f18 {
  color: #999;
}
@media (min-width: 1200px) {
  .f18d {
    margin-left: 2rem;
    padding-left: 2rem;
    border-left: 3px dashed #999;
    text-align: left;
  }
  .f16 {
    text-align: left;
  }
}
@media screen and (min-width: 992px) and (max-width: 1200px) {
  .f16,
  .f18 {
    text-align: center;
  }
}
