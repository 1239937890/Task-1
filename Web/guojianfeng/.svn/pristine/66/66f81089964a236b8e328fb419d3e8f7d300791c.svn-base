main {
  top:4.3rem;
  padding: 0 5%;
  text-align:center;
  background-color:#f0f0f0;
  .back {
    display: block;
    padding: 5% 0;
    margin: 5% 0;
    font-size: 2rem;
    line-height: 2rem;
    cursor: pointer;
    border: 1px solid #c9c9c9;
    border-radius: 5px;
    white-space: nowrap;
    text-decoration: none;
    color: #999;
    background-color: #fafafa;
    b {
      float: right;
      display: inline-block;
      margin-right: 5%;
    }
  }
  .list {
    background-color: #fff;
    border: 1px solid #c9c9c9;
    ul {
      position: relative;
      padding: 0;
      p {
        border-bottom: 1px solid #fdd793;
        font-size: 2rem;
        color: #fbb435;
        padding: 5% 0;
      }
      .next {
        position: absolute;
        top:50.5%;
        right: 2%;
        border-top: 1.7rem solid transparent;
        border-left: 1.7rem solid red;
        border-bottom: 1.7rem solid transparent;
      }
      li {
        list-style: none;
        span {
          font-size: 2rem;
          color: #fff;
          line-height: 5rem;
        }
      }
      .btn {
        background-color: #69d1e9;
        color: #fff;
        margin: 5% 10%;
        font-size: 2rem;
        border: 1px solid #a7e4f2;
      }
      .mag {
        font-size: 3.6%;
        color: #787878;
      }
    }
  }
}
.tab span {
  display: inline-block;
  width: 1rem;
  height: 1rem;
  margin-left: .5rem;
  border-radius: 50%;
  background-color: #f0f0f0;
}
.tab span:first-child {
  background-color: #29bde0;
  margin:0;
}
.back:hover {
  background-color: #eee;
  color: #999;
}

.ack:focus {
  background: darkblue;
  color:#fff;
  position: relative;
  top: 1px;
  text-shadow: none;
  box-shadow: 0 1px 1px rgba(0, 0, 0, .3) inset;
}