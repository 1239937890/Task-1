$header-height: 40px;
$header-fs: 30px;
$header-bc: #29bde0;
$btn-bc: #b7e3ed;
$header-c: #fff;
$btn-width: 40px;
$btn-tac: center;
$btn-imgs: 30px;
$btn-margin: 5px;
body{
  margin: 0;
}
header{
  height: $header-height;
  background-color: $header-bc;
  color:  $header-c;
  width: 100%;
  border-bottom: 1px solid $header-c;
}
.header-center{
  text-align: center;
}
.header-dfbc{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.header-btn{
  height: $header-height;
  width: $btn-width;
  text-align: $btn-tac;
  img{
    width: $btn-imgs;
    height: $btn-imgs;
    margin: 5px;
  }
  a{
    display: inline-block;
    width: $btn-imgs;
    height: $btn-imgs;
    margin: $btn-margin;
    color: $header-c;
    font-size: 24px;
  }
}
.header-title{
  font-size: 24px;
}
.btn-fleft{
  float: left;
}
*{
  box-sizing: border-box;
}
body{
  max-width: 768px;
  min-width: 320px;
  margin: 0 auto;
}

